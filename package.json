{
  "name": "markymark",
  "version": "1.0.0",
  "description": "Stopwatch based on performance.mark() and measure()",
  "main": "lib/markymark.cjs.js",
  "jsnext:main": "lib/markymark.es.js",
  "module": "lib/markymark.es.js",
  "scripts": {
    "clean": "rimraf lib dist && mkdirp lib dist",
    "rollup-polyfills": "rollup -c config/rollup.config.browser.with-polyfills.cjs.js && rollup -c config/rollup.config.iife.with-polyfills.js",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup-iife": "rollup -c config/rollup.config.iife.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es rollup-iife rollup-polyfills",
    "minify": "uglifyjs -mc < dist/markymark.js > dist/markymark.min.js && uglifyjs -mc < dist/markymark.with-polyfills.js > dist/markymark.with-polyfills.min.js",
    "prepublish": "npm run build",
    "build": "npm-run-all clean rollup minify",
    "test": "npm run rollup-cjs && mocha test/test.js",
    "test-browser": "npm run rollup-cjs && zuul --no-coverage test/test.js",
    "dev": "npm run rollup-cjs && NODE_ENV=development zuul --no-coverage --local 9000 test/test.js",
    "lint": "standard"
  },
  "keywords": [
    "mark",
    "measure",
    "performance",
    "user timing",
    "timing",
    "polyfill",
    "stopwatch",
    "timer"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "es6-map": "^0.1.4",
    "lie": "^3.1.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "envify": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.4",
    "zuul": "^3.11.1",
    "zuul-ngrok": "nolanlawson/zuul-ngrok#patch-1"
  },
  "files": [
    "lib",
    "dist"
  ],
  "browser": {
    "./lib/markymark.cjs.js": "./lib/markymark.browser.cjs.js",
    "./lib/markymark.es.js": "./lib/markymark.browser.es.js"
  },
  "standard": {
    "ignore": ["lib", "dist"]
  }
}
